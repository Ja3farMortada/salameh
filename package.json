{
    "name": "salameh",
    "version": "1.0.0",
    "description": "Salameh Cell",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "dist": "electron-builder --x64",
        "publish": "electron-builder --x64 -p always"
    },
    "build": {
        "appId": "com.binariez.Ja3farMortada.salameh",
        "productName": "Salameh Cell app",
        "target": "NSIS",

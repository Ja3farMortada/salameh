        "@sweetalert2/themes": "^5.0.15",
        "angular": "^1.8.3",
        "angular-route": "^1.8.3",
        "angular-utils-pagination": "^0.11.1",
        "bootstrap": "^5.2.3",
        "chart.js": "^4.1.0",
        "electron-log": "^4.4.8",
        "electron-updater": "^5.3.0",
        "exceljs": "^4.3.0",
        "jquery": "^3.6.0",
        "jquery-ui": "^1.13.2",
        "moment": "^2.29.4",

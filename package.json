            {
                "provider": "github",
                "owner": "Ja3farMortada",
                "repo": "salameh"
            }
        ],
        "extraResources": [
            "./server",
            "keys.json"
        ],
        "npmRebuild": "false"
    },
    "repository": {
        "type": "git",
